head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Urban Farmer details
  link(rel='stylesheet' type='text/css' href='../css/style.css')
.container
  #form-wrapper
    form.form-padding(method="GET" action='/dashboardao')
              button.btn.btn-style.btn-dark(type='') Cancel
    form#register-form(action='/registerao' onsubmit="return aovalidate(event);" method='post')
        .container2
            .padding.width
                select#role(name='role')
                    option(value='role') role
                    |                
                    option(value='AgricOfficer') AgricOfficer
                small#roleerr
        .padding
            input#datetoday.form-control-2.width-lg(type='date', name='datetoday', placeholder='date')
            small#datetodayerr
        .padding
            input#names.form-control-2.width-lg(type='text', name='names', placeholder='Fullname')
            small#nameserr
        .container2
            .padding
                select#gender(name='gender')
                    option(value='select your Gender') Select Gender
                    |                 
                    option(value='Male') Male
                    option(value='Female') Female
                    option(value='Secret') Secret
                small#gendererr
        .container3
            .padding
                input#foid.form-control-2(type='text' name='foid' placeholder='AO Id')
                small#foiderr
        .container2  
        .padding
            input#password.form-control-2.width-lg(type='text', name='password', placeholder='Password')
            small#passworderr
        .padding
             button#submit(type='submit') Register
    script(src='/js/Validateao.js')
